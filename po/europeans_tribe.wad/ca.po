#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:3003
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:3010
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:3017
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:3024
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:3031
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:3038
msgctxt "europeans_building"
msgid "Digs coal out of the ground in mountain terrain."
msgstr "Extreu carbó del terra en terreny muntanyós."

#. TRANSLATORS: Purpose helptext for a Europeans production site: Iron Mine
#. TRANSLATORS: Purpose helptext for a Europeans production site: Deep Iron Mine
#. TRANSLATORS: Purpose helptext for a Europeans production site: Deeper Iron Mine
#. TRANSLATORS: Purpose helptext for an Europeans production site: Iron Mine
#. TRANSLATORS: Purpose helptext for an Europeans production site: Deep Iron Mine
#. TRANSLATORS: Purpose helptext for an Europeans production site: Iron Mine
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:3045
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:3052
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:3059
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:3066
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:3073
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:3080
msgctxt "europeans_building"
msgid "Digs iron ore out of the ground in mountain terrain."
