#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:1273
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:1282
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:1291
msgctxt "europeans_worker"
msgid "Lets bees swarm over flowers, then gathers the honey."
msgstr " Lässt Bienen über Blumen ausschwärmen und sammelt ihren Honig."

#. TRANSLATORS: Helptext for an Europeans worker: Miner
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:1303
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:1312
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:1321
msgctxt "europeans_worker"
msgid "Mines ore and minerals."
msgstr "Baut Erze und Mineralien ab."

#. TRANSLATORS: Helptext for an Europeans worker: Terraformer
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:1330
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:1339
#: addons/europeans_tribe.wad/tribes/europeans_tribe/units.lua:1348
